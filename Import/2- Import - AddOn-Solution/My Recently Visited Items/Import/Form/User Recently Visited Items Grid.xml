}

function fixGridHeight()
{
	var el = document.getElementById("treeGrid1");
	top.aras.fixLiquidContainerHeight(document, el);
	el = document.getElementById("grid_td");
	top.aras.fixLiquidContainerHeight(document, el);
}
window.attachEvent("onresize", onresize_handler);	
</script>


